
@{
    ViewBag.Title = "Get";
}
<style>
    .form-horizontal
    {
        margin-top:60px;
    }
</style>

@using (Html.BeginForm())
{

    <div class="form-horizontal">
        <div class="form-group">
            @Html.Label("ValueText", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.Editor("ValueText", new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
    </div>  
   
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Add" class="btn btn-default" />
        </div>
    </div>
   
}

